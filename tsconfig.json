{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es2015",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*",
      ],
    }
  },
  "include": [
    "src",
    "src/types"
  ],
  "exclude": [
    "node_modules"
  ]
}